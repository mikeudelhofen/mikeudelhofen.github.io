@{
    ViewBag.Title = "Mike Udelhofen vs the internet.";
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
</head>
<body>
    <header id="banner">
        <div class="titleWrapper">
            <h1><a class="titleText glow" href="">Mike Udelhofen</a></h1>
        </div>
        <svg id="svgOne" xmlns="http://www.w3.org/2000/svg"></svg>

        <div id="navBar">
            <ul id="nav" data-bind="foreach:navLinks">
                <li class="navLink glow" data-bind="text:displayText, attr:{id:id}"></li>
            </ul>
        </div>
    </header>

    <div id="content"></div>

    <div id="ddContainer">
        <div id="ddBgImages"></div>
        <div id="containerForBgBtns">
            <img src="~/Content/Images/arrow-close.png" id="closeAllImg" class="closeAll hoverHand">
            <img src="~/Content/Images/info-icon.png" class="btnInfo hoverHand">
        </div>
        <label class="lblInfo hide"></label>
    </div>
</body>
</html>
